{
  "cleanUrls": true,
  "routes": [
    {
      "src": "/manifest.json",
      "headers": {
        "Content-Type": "application/manifest+json",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "src": "/sw.js",
      "headers": {
        "Service-Worker-Allowed": "/"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}